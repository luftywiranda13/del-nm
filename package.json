{
  "name": "del-nm",
  "version": "4.0.1",
  "description": "Delete `node_modules` and lockfiles",
  "license": "MIT",
  "repository": "https://github.com/luftywiranda13/del-nm",
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.luftywiranda.com"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "precommit": "remove-lockfiles && lint-staged",
    "test:watch": "jest --watch",
    "test": "xo && jest --coverage"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "force-del": "^2.0.0",
    "has-lockfile": "^4.0.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.3",
    "fixturez": "^1.1.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^7.0.0",
    "remove-lockfiles": "^2.0.4",
    "xo": "^0.29.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "async-to-gen"
    }
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true
  },
  "xo": {
    "envs": [
      "jest"
    ],
    "prettier": true,
    "space": true
  },
  "keywords": [
    "delete",
    "remove",
    "node_modules",
    "npm",
    "yarn",
    "package",
    "dependencies",
    "reinstall"
  ]
}
