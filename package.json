{
  "name": "del-nm",
  "version": "4.0.0",
  "description": "Delete `node_modules` and lockfiles",
  "license": "MIT",
  "repository": "https://github.com/luftywiranda13/del-nm",
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.luftywiranda.com"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "precommit": "remove-lockfiles && xo --fix && lint-staged",
    "test:watch": "jest --watch",
    "test": "xo && jest --coverage"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "force-del": "^1.0.3",
    "has-lockfile": "^4.0.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.3",
    "eslint-config-prettier": "^2.9.0",
    "fixturez": "^1.1.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "remove-lockfiles": "^2.0.4",
    "xo": "^0.20.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "async-to-gen"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "xo": {
    "extends": "prettier",
    "envs": [
      "jest"
    ]
  },
  "keywords": [
    "delete",
    "remove",
    "node_modules",
    "npm",
    "yarn",
    "package",
    "dependencies",
    "reinstall"
  ]
}
